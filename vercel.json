{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/signup", "dest": "/controller/userController.js" },
      { "src": "/login", "dest": "/controller/userController.js" },
      { "src": "/question", "dest": "/controller/questionController.js" },
      { "src": "/question/:id", "dest": "/controller/questionController.js" },
      { "src": "/question/:questionId/testcase", "dest": "/controller/questionController.js" },
      { "src": "/question/:questionId/solution", "dest": "/controller/questionController.js" }
    ]
  }
  