{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/signup", "dest": "/index.js" },
      { "src": "/api/login", "dest": "/index.js" },
      { "src": "/api/question", "dest": "/index.js" },
      { "src": "/api/question/:id", "dest": "/index.js" },
      { "src": "/api/question/:questionId/testcase", "dest": "/index.js" },
      { "src": "/api/question/:questionId/solution", "dest": "/index.js" }
    ]
  }
  